<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow"/>
  <section name="specBind" type="SpecBind.Configuration.ConfigurationSectionHandler, SpecBind"/></configSections>
  <specFlow>
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
  <stepAssemblies>
      <stepAssembly assembly="SpecBind"/>
    </stepAssemblies>
  <unitTestProvider name="nunit"/>
  <plugins>
  <add name="SpecBindGeneratorPlugin" type="Generator"/>
  </plugins>
  </specFlow>
<specBind>
  <application startUrl="http://localhost:8193/"/>
    <!-- For additional details on SpecBind configuration options see the project site. -->
  <browserFactory provider="SpecBind.Selenium.SeleniumBrowserFactory, SpecBind.Selenium" browserType="chrome"/></specBind><startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/></startup></configuration>
